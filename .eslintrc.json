{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true
		},
		"properties": false
	},
	"extends": ["strongloop", "prettier", "prettier/standard"],
	"env": {
		"jest": true,
		"node": true
	},
	"globals": {
		"player_one": "writable",
		"ship_one": "writable",
		"newBoard": "writable",
		"coordinates": "writable"
	},
	"rules": {
		"new-cap": 0
	},
	"ignorePatterns": ["jest.config.js", "./"]
}
